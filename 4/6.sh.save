#!/bin/bash

read string 
if [ ${#string} == 1 ]
then
	echo
	while [ -n "$1" ]
	do
	  case "$1" in
	   [[:upper:]]) echo "UPPER" ;;
	   [[:lower:]]) echo "LOWER" ;;
	   [[:digit:]]) echo "DIGIT" ;;
	   [[:blank:]]) echo "BLANK" ;;
	   [[:punct:]]) echo "PUNCT" ;;
	   [[:space:]]) echo "SPACE"	
	done
else 
	echo "12345"
fi
